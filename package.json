{
  "name": "paragonui-org",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "build:fast": "turbo run build:fast",
    "build:release": "turbo run build --filter=!docs",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:clean": "rm -rf node_modules/.cache/storybook",
    "storybook:serve": "pnpx serve storybook-static",
    "version": "changeset version",
    "release": "changeset publish",
    "version:dev": "changeset version --snapshot dev",
    "release:dev": "changeset publish --tag dev",
    "typecheck": "tsc --noEmit",
    "lint": "eslint packages/**/**/src --ext .ts,.tsx --config .eslintrc.json --cache",
    "format:check": "prettier --check packages/**/**/src --cache",
    "format:write": "prettier --write packages/**/**/src --cache",
    "clean": "pnpm -r --parallel exec rimraf dist .turbo *.log node_modules .next",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "postinstall": "tsx scripts/patchJestDom.ts",
    "changeset": "changeset"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.1",
    "@paragonui-org/eslint-config": "workspace:*",
    "@storybook/addon-a11y": "^7.0.4",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/addon-storysource": "^6.5.15",
    "@storybook/blocks": "^7.0.2",
    "@storybook/cli": "^7.0.4",
    "@storybook/react": "^7.0.2",
    "@storybook/react-vite": "^7.0.2",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/theming": "^6.5.15",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^17.0.12",
    "@types/react": "^18.0.22",
    "@types/testing-library__jest-dom": "5.14.5",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.30.0",
    "happy-dom": "^9.1.7",
    "prettier": "^2.6.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "storybook": "^7.0.2",
    "tsup": "^6.7.0",
    "tsx": "^3.12.6",
    "turbo": "latest",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vitest": "^0.30.0"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  },
  "engines": {
    "node": ">=16.x"
  },
  "packageManager": "pnpm@8.1.0"
}
